{
  "version": 2,
  "builds": [
    {
      "src": "back/api/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back/api/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "front/$1"
    }
  ]
}